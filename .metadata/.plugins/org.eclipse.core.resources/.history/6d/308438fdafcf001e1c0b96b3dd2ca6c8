package org.kh.java;

public class berryvery {

	public static void main(String[] args) {
		char ch6 = '\u26BD';
		System.out.println("ch6 : "+ch6);
		
		float f1 = 3.14f;
		double f2 = 3.14;
		
		System.out.printf("\nf1=%f", f1);
		int kor=90, eng=80, mat=95;
		float avg = (kor + eng + mat) / 3;
		System.out.printf("\n평균 : %.2f", avg);
		

	}

}
